<div class="container-fluid h-100">
  <div class="row h-100">
    <div class="col-5 col-md-3 collapse width m-0 p-0 h-100 bg-dark" id="collapseExample">
       ..
    </div>
    <div class="col">
      <div class="row">
        <div class="col-12">
          <button class="btn sticky-top" data-toggle="collapse" href="#collapseExample" role="button">
            <div class="top-links">
            <!-- Your header content here, e.g., logo and navigation menu -->
               <% if customer_signed_in? %>
                <%= button_to "logout", destroy_customer_session_path, method: :delete, class: "nav-link" %>
               <% else %>
                <%= link_to "Sign Up", new_customer_registration_path, class: "nav-link" %>
                <%= link_to "Login", new_customer_session_path, class: "nav-link" %>
               <% end %>
            </div>
          </button>
        </div>
        <div class="col-12">
        </div>
      </div>
    </div>
  </div>
</div>
